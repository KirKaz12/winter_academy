- pageLang = 'ru'
- pageTime = new Date().getTime()
- pageTitle = 'Winter Academy'
- images = "./images/"

block variables
  - variable = null

doctype html
html.app-nojs(lang= pageLang, data-prefix="app")
  head
    title= pageTitle
    meta(charset='utf-8')
    meta(content='width=device-width, initial-scale=1.0, minimal-ui' name='viewport')
    meta(content='address=no' name='format-detection')
    meta(content='telephone=no' name='format-detection')
    link(href='./styles/common.css?' + pageTime rel='stylesheet')
    script(src='./scripts/init.min.js')
  body
    .loader.js-loading
    .layout
      header.header
        .header_primary
          .header_primary_heading.offset
            .header_primary_heading_holder.holder
              .header_logo
                a(href="#")
                  <svg class="icon"><use xlink:href="#icon-logo"></use></svg>
              nav.header_nav
                ul.header_nav_list
                  li: a.js-scroller(href="#about", data-target="#about") Об академии
                  li: a.js-scroller(href="#teachers", data-target="#teachers") Преподаватели
                  li: a.js-scroller(href="#shedule", data-target="#shedule") Учебный план
                  li: a.js-scroller(href="#register", data-target="#register") Стоимость курса
                  li: a.js-scroller(href="#discount", data-target="#discount") Скидки и бонусы
          .header_intro.offset
            .header_intro_holder.holder
              .header_intro_offset
                .header_intro_content
                  h1.header_intro_title Самый ожидаемый финансовый курс <em>2016<span>года</span></em>
                  .header_intro_description За 6 месяцев мы раскроем опыт работы в инвестиционнах банках, корпорациях, венчурных и хедж-фондах, а также поделимся знаниями, приобретенными в лучших университетах мира
                .header_intro_badge
                  .header_intro_badge_icon
                    svg.icon
                      use(xlink:href="#icon-tie")
                  ul.header_intro_badge_meta
                    li: span Старт Зимней<br> Академии
                    li: span 3
                    li: span декабря
        .header_secondary.offset
          .header_secondary_holder.holder
